
EquationLeft(_symbolicEquation)_(Type(symbolicEquation) = "==")  <--
[
    Local(listForm);
    
    listForm := FunctionToList(symbolicEquation);
    
    listForm[2];
];

EquationRight(_symbolicEquation)_(Type(symbolicEquation) = "==")  <--
[
    Local(listForm);
    
    listForm := FunctionToList(symbolicEquation);
    
    listForm[3];
];

